{
  "name": "cathub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:cathub": "node dist/cathub/server/server.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name migrate",
    "postinstall": "prisma generate"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.2",
    "@prisma/client": "^6.18.0",
    "@tailwindcss/postcss": "^4.1.14",
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fastest-validator": "^1.19.1",
    "jalali-moment": "^3.3.11",
    "jsonwebtoken": "^9.0.2",
    "material-icons": "^1.13.14",
    "memory-cache-map": "^1.1.1",
    "multer": "^2.0.2",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "swiper": "^12.0.2",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@inquirer/prompts": "^7.9.0",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.17.19",
    "commander": "^14.0.2",
    "daisyui": "^5.3.6",
    "prisma": "^6.18.0",
    "typescript": "~5.9.2"
  }
}
